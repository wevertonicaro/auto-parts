{
   "name": "auto-parts",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "build": "tsc -p tsconfig.json",
      "dev": "ts-node-dev -r tsconfig-paths/register --inspect --log-error --transpile-only --ignore-watch node_modules --respawn src/http/main/main.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/shared/infra/typeorm/database/dataSource.ts",
      "revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/shared/infra/typeorm/database/dataSource.ts",
      "migration": "sh -c 'typeorm migration:create ./src/shared/infra/typeorm/migrations/$0'",
      "entity": "sh -c 'typeorm entity:create ./src/shared/infra/typeorm/entities/$0'",
      "formatter": "npx prettier 'src/**/*.{js,ts,mjs,cjs,json}' --write",
      "check": "npx prettier . --check",
      "start": "ts-node dist/src/http/main/main.js",
      "seed": "ts-node src/shared/infra/typeorm/seeds/index.ts",
      "lint": "npx eslint 'src/**/*.{js,ts}' --fix",
      "test": "jest",
      "test-dev": "jest --watch",
      "test:coverage": "jest --coverage"
   },
   "dependencies": {
      "@redis/client": "^1.6.0",
      "@types/redis": "^4.0.11",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.20.3",
      "class-transformer": "^0.5.1",
      "cors": "^2.8.5",
      "csv-parser": "^3.0.0",
      "dotenv": "^16.4.5",
      "express": "^4.21.1",
      "express-async-error": "^0.0.2",
      "express-async-errors": "^3.1.1",
      "jsonwebtoken": "^9.0.2",
      "logger": "^0.0.1",
      "moment-timezone": "^0.5.46",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^3.11.4",
      "rate-limiter-flexible": "^5.0.4",
      "redis": "^4.7.0",
      "reflect-metadata": "^0.2.2",
      "sqlite3": "^5.1.7",
      "swagger-tools": "^0.9.8",
      "swagger-ui-express": "^5.0.1",
      "tsyringe": "^4.8.0",
      "typeorm": "0.3.20",
      "update": "^0.4.2",
      "winston": "^3.16.0",
      "xlsx": "^0.18.5",
      "yup": "^1.4.0"
   },
   "devDependencies": {
      "@jest/globals": "^29.7.0",
      "@types/bcryptjs": "^2.4.6",
      "@types/cors": "^2.8.17",
      "@types/express": "^5.0.0",
      "@types/jest": "^29.5.14",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/moment": "^2.13.0",
      "@types/morgan": "^1.9.9",
      "@types/multer": "^1.4.12",
      "@types/node": "^22.9.0",
      "@types/swagger-tools": "^0.10.10",
      "@types/swagger-ui-express": "^4.1.7",
      "jest": "^29.7.0",
      "prettier": "3.3.3",
      "ts-jest": "^29.2.5",
      "ts-loader": "^9.5.1",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.7.2"
   }
}
